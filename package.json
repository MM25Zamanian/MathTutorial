{
  "name": "math-tutorial-ts",
  "version": "0.0.0",
  "description": "Math Tutorial With TypeScript",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve",
    "serve": "cd dist && node index.js",
    "build": "yarn build-ts",
    "build-ts": "rm -rf dist && tsc --build .",
    "watch-node": "nodemon --watch dist ",
    "watch-node-debug": "nodemon --watch dist --inspect ",
    "watch-ts": "rm -rf dist && tsc -w"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "nodemon": "^2.0.12",
    "typescript": "^4.1.3"
  }
}
